fn main() i32 {
    if false || false {
        return 1;
    }

    if false && false {
        return 1;
    }

    if false && true {
        return 1;
    }

    if !(false || true) {
        return 1;
    }

    if false && core::exit(1) {
        return 1;
    }

    if true || core::exit(1) {
        return 0;
    }

    return 1;
}
