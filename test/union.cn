#(untagged)
union Untagged {
    a: i32,
    b: i64,
}

union Tagged {
    a: i32,
    b: i64,
}

fn main() i32 {
    mut u = Untagged { a: 1 };
    core::assert(u.a == 1);

    u = Untagged { b: 42 };
    core::assert(u.b == 42);

    u = Untagged { a: u.b as i32 };
    core::assert(u.a == 42);

    mut t = Tagged { a: 1 };
    core::assert(t.a == 1);

    t = Tagged { b: 42 };
    core::assert(t.b == 42);

    switch t {
        Tagged::a { return 1; }
        Tagged::b { return 0; }
        else { return 42; }
    }
}
